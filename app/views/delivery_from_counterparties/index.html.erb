<div class="container my-5">
  <div class="card shadow-lg">
    <div class="card-header d-flex justify-content-between align-items-center">
      <div class="btn-group" role="group" aria-label="Basic example">
        <button type="button" class="btn btn-primary">Общая информация</button>
        <%= link_to "Подробнее", product_entries_path, class: 'btn btn-secondary' %>
      </div>
    </div>
    <%= render 'filter_form', q: @q, url: request.path %>
    <div class="card-body">
      <h1 class='text-center'>История поступления товаров</h1>
      <%= render 'delivery_from_counterparties', delivery_from_counterparties: @delivery_from_counterparties %>
      <%= paginate @delivery_from_counterparties %>
    </div>
  </div>
</div>
